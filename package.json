{
  "name": "async-iterable-prefetch",
  "version": "0.0.0",
  "description": "Creates a prefetched async iterable",
  "homepage": "http://github.com/voxpelli/async-iterable-prefetch",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/async-iterable-prefetch.git"
  },
  "type": "module",
  "exports": "./index.js",
  "types": "index.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.d.ts.map",
    "index.js",
    "index.d.ts",
    "index.d.ts.map"
  ],
  "scripts": {
    "benchmark:event-loop-lag": "node benchmark/event-loop-lag.js",
    "benchmark:memory": "node --expose-gc benchmark/memory.js",
    "benchmark": "run-s benchmark:*",
    "build:0": "run-s clean",
    "build:1-declaration": "tsc -p declaration.tsconfig.json",
    "build": "run-s build:*",
    "check:dependency-check": "dependency-check *.js 'lib/**/*.js' 'test/**/*.js' --no-dev",
    "check:installed-check": "installed-check -i eslint-plugin-jsdoc",
    "check:lint": "eslint .",
    "check:tsc": "tsc",
    "check:type-coverage": "typescript-coverage-report --strict --threshold 95 -i 'benchmark/**/*.js'",
    "check": "run-s clean && run-p check:*",
    "clean:declarations": "rm -rf $(find . -maxdepth 2 -type f -name '*.d.ts*')",
    "clean": "run-p clean:*",
    "flame:real:event-loop-immediate": "0x -o -- node benchmark/event-loop-lag-real-immediate.js",
    "flame:real:event-loop-static": "0x -o -- node benchmark/event-loop-lag-real-static.js",
    "flame:real": "run-s flame:real:*",
    "flame:static:event-loop-immediate-all": "0x -o -- node benchmark/event-loop-lag-immediate-all.js",
    "flame:static:event-loop-immediate-tenth": "0x -o -- node benchmark/event-loop-lag-immediate-tenth.js",
    "flame:static:event-loop-static": "0x -o -- node benchmark/event-loop-lag-static.js",
    "flame:static": "run-s flame:static:*",
    "flame": "run-s flame:*",
    "prepublishOnly": "run-s build",
    "sync-gh-actions": "ghat",
    "test:mocha": "c8 --reporter=lcov --reporter text mocha 'test/**/*.spec.js'",
    "test-ci": "run-s test:*",
    "test": "run-s check test:*",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "devDependencies": {
    "@airbnb/node-memwatch": "^2.0.0",
    "@types/airbnb__node-memwatch": "^2.0.0",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.21",
    "@types/sinon": "^10.0.11",
    "@types/sinon-chai": "^3.2.8",
    "@voxpelli/eslint-config": "^14.1.0",
    "@voxpelli/tsconfig": "^4.0.0",
    "0x": "^5.3.1",
    "c8": "^7.11.3",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-quantifiers": "^1.0.16",
    "dependency-check": "^5.0.0-7",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "ghat": "^0.14.0",
    "husky": "^8.0.1",
    "installed-check": "^6.0.1",
    "mocha": "^10.0.0",
    "npm-run-all2": "^6.0.1",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "typescript": "~4.6.0",
    "typescript-coverage-report": "^0.6.4"
  }
}
