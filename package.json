{
  "name": "buffered-async-iterable",
  "version": "0.0.0",
  "description": "Creates a buffered async iterable",
  "homepage": "http://github.com/voxpelli/buffered-async-iterable",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/buffered-async-iterable.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "benchmark:event-loop-lag": "node benchmark/event-loop-lag.js",
    "benchmark:memory": "node --expose-gc benchmark/memory.js",
    "benchmark": "run-s benchmark:*",
    "build:0": "run-s clean",
    "build:1-declaration": "tsc -p declaration.tsconfig.json",
    "build": "run-s build:*",
    "check:dependency-check": "dependency-check *.js 'test/**/*.js' --no-dev",
    "check:installed-check": "installed-check",
    "check:lint": "eslint .",
    "check:tsc": "tsc",
    "check": "run-s clean && run-p check:*",
    "clean:declarations": "rm -rf $(find . -maxdepth 2 -type f -name '*.d.ts')",
    "clean": "run-p clean:*",
    "flame:real:event-loop-immediate": "0x -o -- node benchmark/event-loop-lag-real-immediate.js",
    "flame:real:event-loop-static": "0x -o -- node benchmark/event-loop-lag-real-static.js",
    "flame:real": "run-s flame:real:*",
    "flame:static:event-loop-immediate-all": "0x -o -- node benchmark/event-loop-lag-immediate-all.js",
    "flame:static:event-loop-immediate-tenth": "0x -o -- node benchmark/event-loop-lag-immediate-tenth.js",
    "flame:static:event-loop-static": "0x -o -- node benchmark/event-loop-lag-static.js",
    "flame:static": "run-s flame:static:*",
    "flame": "run-s flame:*",
    "prepublishOnly": "run-s build",
    "sync-gh-actions": "ghat",
    "test:mocha": "c8 --reporter=lcov --reporter text mocha 'test/**/*.spec.js'",
    "test-ci": "run-s test:*",
    "test": "run-s check test:*"
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "0BSD",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@airbnb/node-memwatch": "^2.0.0",
    "@types/airbnb__node-memwatch": "^2.0.0",
    "@types/chai": "^4.2.16",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.20.7",
    "@types/sinon": "^9.0.11",
    "@types/sinon-chai": "^3.2.5",
    "@voxpelli/eslint-config": "11.0.1",
    "@voxpelli/eslint-config-jsdoc-ts": "^0.3.1",
    "@voxpelli/tsconfig": "^2.1.0",
    "0x": "^4.10.2",
    "c8": "^7.7.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dependency-check": "^4.1.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.13",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "ghat": "^0.14.0",
    "installed-check": "^5.0.0-0",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "typescript": "^4.3.5"
  }
}
